@model List<HotelAtr.DAL.RoomType>

@{
    ViewBag.Title = "RoomTypeList";
    Layout = "~/Views/Shared/_LayoutContent.cshtml";
}
<div class="row">
    <div class="col-md-3"></div>
    <div class="col-md-6">
        <br />
        <br />
        <h2>RoomTypeList</h2>
        <div class="form-body">
            <p>
                @Html.ActionLink("Create New", "CreateRoomType")
            </p>
            <table class="table">
                <tr>
                    <th>Description</th>
                    <th></th>
                </tr>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Description)
                        </td>
                        <td>
                            @Html.ActionLink("Edit", "EditRoomType", new { id = item.RoomTypeId }) |
                            @Html.ActionLink("Details", "DetailsRoomType", new { id = item.RoomTypeId }) |
                        </td>
                    </tr>
                }
            </table>
        </div>
        <br />
        <br />
    </div>
    <div class="col-md-3"></div>
</div>